{
  "key": "",
  "manifest_version": 2,
  "name": "Automated Tab Organization",
  "description": "A WebExtension for automated tab organization",
  "version": "0.0.1",
  "incognito": "not_allowed",
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "nativeMessaging",
    "notifications",
    "contextMenus"
  ],
  "browser_action": {
    "default_popup": "ui.html"
  },
  "background": {
    "scripts": ["background.bundle.js"]
  },
  "chrome_url_overrides": {
    "newtab": "ui.html"
  },
  "options_ui": {
    "chrome_style": true,
    "page": "options.html",
    "open_in_tab": false
  },
  "applications": {
    "gecko": {
      "id": "automated-tab-organization@rschlaefli",
      "strict_min_version": "50.0"
    }
  },
  "web_accessible_resources": ["ui.html", "tutorial.html"]
}
