    "@types/react-redux": "7.1.9",
    "@types/uuid": "8.0.0",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "autoprefixer": "9.8.0",
    "awesome-typescript-loader": "5.2.1",
    "axios": "0.19.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
