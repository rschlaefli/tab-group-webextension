    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "1.13.0",
    "blueimp-md5": "2.16.0",
    "clean-webpack-plugin": "3.0.0",
    "clsx": "1.1.0",
    "copy-webpack-plugin": "5.1.1",
    "cross-env": "7.0.2",
    "css-loader": "3.5.3",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.2",
