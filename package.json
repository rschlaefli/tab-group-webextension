  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(json|css|md)": [
      "prettier --write"
    ],
    "*.+(js|jsx|tsx|ts)": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 chrome version"
    ],
    "production": [
      "last 5 chrome versions"
    ]
  }
}
