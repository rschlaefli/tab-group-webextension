{
  "name": "automated-tab-organization",
  "version": "0.0.1",
  "author": "Roland Schlaefli <rolandschlaefli@gmail.com>",
  "description": "A WebExtension for automated tab organization",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rschlaefli/automated-tab-organization.git"
  },
  "scripts": {
    "build": "npm-run-all build:style build:app build:package",
    "build:app": "cross-env NODE_ENV=production webpack",
    "build:style": "postcss src/styles/index.css -o src/styles/tailwind.css",
    "build:package": "zip -r `date +%Y-%m-%d`.zip build",
    "dev": "npm-run-all --parallel dev:style dev:app",
    "dev:app": "cross-env NODE_ENV=development nodemon --delay 2500ms -e json,html -w public -w package.json -w package-lock.json -w webpack.config.js --exec 'webpack -w'",
    "dev:style": "postcss src/styles/index.css -o src/styles/tailwind.css --watch",
    "dev:serve": "cross-env NODE_ENV=development nodemon --delay 2500ms -e json,html -w public -w package.json -w package-lock.json -w webpack.config.js --exec 'webpack-dev-server'",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\" && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "format:ci": "prettier --check \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}'",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch"
  },
  "dependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-typescript": "7.9.0",
    "@fullhuman/postcss-purgecss": "2.2.0",
    "@material-ui/core": "4.9.14",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.53",
    "@reduxjs/toolkit": "1.3.6",
    "@svgr/webpack": "5.4.0",
    "@types/chrome": "0.0.112",
    "@types/jest": "25.2.2",
    "@types/node": "14.0.1",
    "@types/ramda": "0.27.4",
    "@types/react": "16.9.35",
    "@types/react-beautiful-dnd": "12.1.2",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.8",
    "@types/uuid": "7.0.3",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "autoprefixer": "9.7.6",
    "awesome-typescript-loader": "5.2.1",
    "axios": "0.19.2",
    "babel-core": "6.26.3",
