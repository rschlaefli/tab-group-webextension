{
  "name": "automated-tab-organization",
  "version": "0.2.1",
  "author": "Roland Schlaefli <rolandschlaefli@gmail.com>",
  "description": "A WebExtension for automated tab organization",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rschlaefli/automated-tab-organization.git"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "npm-run-all build:style build:app",
    "build:all": "npm-run-all --parallel build:chrome build:firefox",
    "build:analyze": "cross-env ANALYZE=true npm-run-all build:style build:app",
    "build:app": "cross-env NODE_ENV=production webpack --env.production --env.browser=$BROWSER --env.analyze=$ANALYZE",
    "build:chrome": "cross-env BROWSER=chrome npm run build:app",
    "build:edge": "cross-env BROWSER=edge npm run build:app",
    "build:firefox": "cross-env BROWSER=firefox npm run build:app",
    "build:opera": "cross-env BROWSER=opera npm run build:app",
    "build:style": "cross-env NODE_ENV=production postcss src/styles/index.css -o src/styles/tailwind.css",
    "deploy": "npm-run-all build:style build:all package:all package:sources",
    "deploy:chrome": "npm-run-all build:style build:chrome package:chrome",
    "deploy:edge": "npm-run-all build:style build:edge package:edge",
    "deploy:firefox": "npm-run-all build:style build:firefox package:firefox",
    "deploy:opera": "npm-run-all build:style build:opera package:opera",
    "dev": "npm-run-all --parallel dev:style dev:app",
    "dev:app": "cross-env NODE_ENV=development nodemon --delay 2500ms -e json,html -w public -w package.json -w package-lock.json -w webpack.config.js --exec 'webpack --watch'",
    "dev:style": "postcss src/styles/index.css -o src/styles/tailwind.css --watch",
    "dev:serve": "cross-env NODE_ENV=development nodemon --delay 2500ms -e json,html -w public -w package.json -w package-lock.json -w webpack.config.js --exec 'webpack-dev-server'",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\" && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "format:ci": "prettier --check \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "lint": "npm run type-check && eslint '*/**/*.{js,ts,tsx}'",
    "package:all": "npm-run-all --parallel package:chrome package:firefox",
    "package:chrome": "zip -jo `date +%Y-%m-%d`-chrome.zip build/chrome/*",
    "package:edge": "zip -jo `date +%Y-%m-%d`-edge.zip build/edge/*",
    "package:firefox": "zip -jo `date +%Y-%m-%d`-firefox.xpi build/firefox/*",
    "package:opera": "zip -jo `date +%Y-%m-%d`-opera.zip build/opera/*",
    "package:sources": "zip -o `date +%Y-%m-%d`-sources.zip public/**/* src/**/* .eslintignore .eslintrc.js .prettierignore .prettierrc.js babel.config.js jest.config.js manifest.json package-lock.json package.json postcss.config.js README.md tailwind.config.js tsconfig.json webpack.config.js",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-optional-chaining": "7.10.1",
    "@babel/plugin-proposal-pipeline-operator": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "@babel/preset-typescript": "7.10.1",
    "@material-ui/core": "4.10.1",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.55",
    "@reduxjs/toolkit": "1.3.6",
    "@svgr/webpack": "5.4.0",
    "@types/chrome": "0.0.114",
